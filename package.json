    "browser-sync": "2.18.8",
    "chai": "3.5.0",
    "chokidar": "1.6.1",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.27.1",
    "enzyme": "2.7.1",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-import-resolver-webpack": "0.8.1",
    "eslint-loader": "1.6.3",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.0",
    "file-loader": "0.10.1",
